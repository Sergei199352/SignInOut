<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    
    <title>NFC deata</title>
</head>
<body>
    <h1>Post Data:</h1>
    
    <% if (ressql) { %>
        
      

      <% for (let i = 0; i < ressql.recordsets.length; i++) { %>
        <% for (let j = 0; j < ressql.recordsets[i].length; j++) { %>
         <div id=<%= nfc %>> <h2>Record <%= i + 1 %>, Object <%= j + 1 %></h2>
          <ul>
            <% const recordset = ressql.recordsets[i][j]; %>
            <% for (let key in recordset) { %>
              <li><%= key %>: <%= recordset[key] %></li>
            <% } %>
          </ul> </div>
        <% } %>
      <% } %>

      <% for (let i = 0; i < recordset.recordsets.length; i++) { %>
        <% for (let j = 0; j < recordset.recordsets[i].length; j++) { %>
         <div id=<%= nfc %>> <h2>Record <%= i + 1 %>, Object <%= j + 1 %></h2>
          <ul>
            <% const recordset = recordset.recordsets[i][j]; %>
            <% for (let key in recordset) { %>
              <li><%= key %>: <%= recordset[key] %></li>
            <% } %>
          </ul> </div>
        <% } %>
      <% } %>
      
    <% } else { %>
      <p>No data posted yet.</p>
    <% } %>
</body>
</html>