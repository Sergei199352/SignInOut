<!-- upload.ejs -->

<!DOCTYPE html>
<html>
<head>
  <title>CSV File Upload</title>
</head>
<body>
  <h1>CSV File Upload</h1>

  <form action="/upload" method="POST" enctype="multipart/form-data">
    <input type="file" name="file" accept=".csv" required>
    <input type="submit" value="Upload">
  </form>

  
</body>
</html>
